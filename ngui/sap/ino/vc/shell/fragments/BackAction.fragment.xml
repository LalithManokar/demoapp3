<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <ActionSheet
		showCancelButton="true"
		placement="Bottom"
		buttons="{history>/HashList}"
		>
		<buttons>
		    <Button class="sapInoHistoricMenuBtn" text="{parts:['history>title','history>hash'], formatter:'.formatter.historyMenuDetail'}" press="onBackMenuPress" tooltip="{parts:['history>title','history>hash'], formatter:'.formatter.historyMenuDetail'}"/>
		</buttons>
	</ActionSheet>     
</core:FragmentDefinition>