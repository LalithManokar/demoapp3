<mvc:View
    controllerName="sap.ino.vc.idea.Evaluations"
	xmlns="sap.m"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout">
	<l:Grid id="myGrid" defaultSpan="XL12 L12 M12 S12">
		<List
            visible="{= (${object>/property/nodes/Root/customProperties/backofficeEvalReqPrivilege} &amp;&amp; ${view>/EVALUATION_REQUEST_LIST_DISPLAY}) }"
            class="sapInoEvaluationList"            
            headerText="{i18n>EVALUATION_LIST_TIT}"
            id="evalationRequestManager"
            items="{path: 'data>EvaluationRequests',
                    sorter: {path: 'CREATED_AT',
                              descending: true}}"
            mode="None"
            noDataText="{i18n>EVALUATION_LBL_NO_REQUEST_FOUND}">
			<headerToolbar>
				<Toolbar width="100%">
					<content>
						<Title
                            class="sapUxAPObjectPageSubSectionHeaderTitle"
                            text="{i18n>EVALUATION_OBJECT_ALL_REQUESTS}"/>
					</content>
				</Toolbar>
			</headerToolbar>
			<CustomListItem
                id="evalationRequestListItemManager"
                press="onItemRequestPressManager"
                type="Active">
				<content>
					<l:Grid
                        class="sapUiSmallMarginTopBottom sapInoEvaluationListItemMiddle"
                        defaultSpan="XL12 L12 M9 S8"
                        hSpacing="0"
                        vSpacing="0">
						<l:content>
							<HBox>
								<VBox>
									<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>CREATED_AT', formatter: '.formatter.toDate'}"></Text>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL2 L2 M12 S12"/>
								</layoutData>
							</HBox>
							<HBox>
								<VBox visible="{= ${data>EVALUATOR_ID} > 0}">
									<Link class="sapInoEvaluationListEvaluatorName"
                                    text="{data>OWNER_NAME}"
                                    press="onEvaluationRequestOwnerPressed"></Link>
								</VBox>
								<VBox visible="{= ${data>EVALUATOR_ID} === 0}">
									<Text text="{i18n>IDEA_OBJECT_TIT_IDEA_Anonymity}"/>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL2 L2 M12 S12"/>
								</layoutData>
							</HBox>
							<HBox>
								<VBox class="sapUiSmallMarginBegin">
									<Label
                                class="sapInoEvaluationListLabel"
                                text="{i18n>EVALUATION_OBJECT_REQUEST_ACC_DATE}"></Label>
									<layoutData>
										<l:GridData
                                            span="XL2 L2 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox class="sapUiSmallMarginBegin">
									<Text  class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>ACCEPT_DATE', formatter: '.formatter.toDate'}"></Text>
									<layoutData>
										<l:GridData
                                            span="XL1 L1 M12 S12"/>
									</layoutData>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL3 L3 M12 S12"/>
								</layoutData>
							</HBox>
							<HBox>
								<VBox class="sapUiSmallMarginBegin">
									<Label
                                class="sapInoEvaluationListLabel"
                                text="{i18n>EVALUATION_OBJECT_REQUEST_COM_DATE}"></Label>
									<layoutData>
										<l:GridData
                                            span="XL2 L2 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox class="sapUiSmallMarginBegin">
									<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>COMPLETE_DATE', formatter: '.formatter.toDate'}"></Text>
									<layoutData>
										<l:GridData
                                            span="XL1 L1 M12 S12"/>
									</layoutData>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL3 L3 M12 S12"/>
								</layoutData>
							</HBox>
						</l:content>
					</l:Grid>
				</content>
			</CustomListItem>
		</List>
		<List
            visible="{= (!(${object>/property/nodes/Root/customProperties/backofficeEvalReqPrivilege}) &amp;&amp; ${object>/property/nodes/Root/customProperties/frontofficeEvalReqPrivilege} &amp;&amp; ${view>/EVALUATION_REQUEST_LIST_DISPLAY})}"
            class="sapInoEvaluationList"   
            headerText="{i18n>EVALUATION_LIST_TIT}"
            id="evalationRequestExpert"
            items="{path: 'data>EvaluationRequestItems',
                    sorter: {path: 'CREATED_AT',
                              descending: true}}"
            mode="None"
            noDataText="{i18n>EVALUATION_LBL_NO_REQUEST_FOUND}">
			<headerToolbar>
				<Toolbar width="100%">
					<content>
						<Title
                            class="sapUxAPObjectPageSubSectionHeaderTitle"
                            text="{i18n>EVALUATION_OBJECT_MY_REQUESTS}"/>
					</content>
				</Toolbar>
			</headerToolbar>
			<CustomListItem
                id="evalationRequestListItemExpert"
                press="onItemRequestPressExpert"
                type="Active">
				<content>
					<l:Grid
                        class="sapUiSmallMarginTopBottom sapInoEvaluationListItemMiddle"
                        defaultSpan="XL12 L12 M9 S8"
                        hSpacing="0"
                        vSpacing="0">
						<l:content>
							<HBox>
								<VBox>
									<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>CREATED_AT', formatter: '.formatter.toDate'}"></Text>
								</VBox>
								<VBox class="sapUiSmallMarginBegin">
									<Link class="sapInoEvaluationListEvaluatorName"
                                    text="{data>OWNER_NAME}"
                                    press="onEvaluationRequestOwnerPressed"></Link>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL3 L3 M12 S12"/>
								</layoutData>
							</HBox>
							<HBox>
								<VBox class="sapUiSmallMarginBegin">
									<Label
                                class="sapInoEvaluationListLabel"
                                text="{i18n>EVALUATION_OBJECT_REQUEST_ACC_DATE}"></Label>
									<layoutData>
										<l:GridData
                                            span="XL2 L2 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox class="sapUiSmallMarginBegin">
									<Text  class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>ACCEPT_DATE', formatter: '.formatter.toDate'}"></Text>
									<layoutData>
										<l:GridData
                                            span="XL1 L1 M12 S12"/>
									</layoutData>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL3 L3 M12 S12"/>
								</layoutData>
							</HBox>
							<HBox>
								<VBox class="sapUiSmallMarginBegin">
									<Label
                                class="sapInoEvaluationListLabel"
                                text="{i18n>EVALUATION_OBJECT_REQUEST_COM_DATE}"></Label>
									<layoutData>
										<l:GridData
                                            span="XL2 L2 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox class="sapUiSmallMarginBegin">
									<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>COMPLETE_DATE', formatter: '.formatter.toDate'}"></Text>
									<layoutData>
										<l:GridData
                                            span="XL1 L1 M12 S12"/>
									</layoutData>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL3 L3 M12 S12"/>
								</layoutData>
							</HBox>
							<HBox>
								<VBox  class="sapUiSmallMarginBegin">
									<Label
                                class="sapInoEvaluationListLabel"
                                text="{i18n>EVALUATION_OBJECT_REQUEST_STATUS}"></Label>
								</VBox>
								<VBox class="sapUiSmallMarginBegin">
                                    <Text class="sapInoEvaluationListItemDate"
                                     text="{path: 'data>STATUS_CODE', formatter: '.formatter.ideaStatus'}"> </Text>
								</VBox>
								<layoutData>
									<l:GridData
                                            span="XL2 L2 M12 S12"/>
								</layoutData>
							</HBox>
						</l:content>
					</l:Grid>
				</content>
			</CustomListItem>
		</List>
		<List
            class="sapInoEvaluationList sapUiMediumMarginBottom"
            headerText="{i18n>EVALUATION_LIST_TIT}"
            id="evaluationList"
            items="{path: 'data>Evaluations',
                    sorter: [{path: 'GROUP_ORDER',
                              descending: false,
                              group: '.grouper'},
                             {path: 'CHANGED_AT',
                              descending: true}],
                    groupHeaderFactory: '.formatter.formatGroupHeader'}"
            mode="None"
            noDataText="{i18n>EVALUATION_LBL_NO_EVALUATIONS_FOUND}"
            visible="{object>/property/nodes/Root/customProperties/backofficeCampaignPrivilege}">
			<headerToolbar>
				<Toolbar width="100%">
					<content>
						<Title
                            class="sapUxAPObjectPageSubSectionHeaderTitle"
                            text="{i18n>EVALUATION_OBJECT_ALL}"/>
					</content>
				</Toolbar>
			</headerToolbar>
			<CustomListItem
                id="evaluationListItem"
                press="onItemPress"
                type="Active">
				<content>
					<l:Grid
                        class="sapUiSmallMarginTopBottom sapInoEvaluationListItemMiddle"
                        defaultSpan="XL8 L8 M8 S8"
                        hSpacing="0"
                        vSpacing="0">
						<l:content>
							<l:Grid
                            class="sapInoEvaluationListItemMiddle"
                            defaultSpan="XL8 L8 M12 S12"
                            hSpacing="0"
                            vSpacing="0">
								<l:content>
									<VBox>
										<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>CHANGED_AT', formatter: '.formatter.toDate'}"></Text>
										<Link visible="{= ${data>EVALUATOR_ID} > 0}"
                                    class="sapInoEvaluationListEvaluatorName"
                                    text="{data>EVALUATOR_NAME}"
                                    press="onEvaluatorPressed"></Link>
										<Text visible="{= ${data>EVALUATOR_ID} === 0}"
                                    text="{i18n>IDEA_OBJECT_TIT_IDEA_Anonymity}"></Text>
										<layoutData>
											<l:GridData
                                            span="XL4 L4 M12 S12"/>
										</layoutData>
									</VBox>
									<l:Grid
                                    defaultSpan="XL6 L6 M12 S12"
                                    vSpacing="0"
                                    hSpacing="0">
										<l:content>
											<Label
                                            class="sapInoEvaluationListLabel"
                                            text="{i18n>EVALUATION_TIT_EVAL_METHOD}">
												<layoutData>
													<l:GridData
                                                    span="XL4 L4 M12 S12"
                                                    visibleS="false"/>
												</layoutData>
											</Label>
											<Text text="{path: 'data>MODEL_CODE', formatter: '.formatter.modelCode'}"/>
											<Label
				                            class="sapInoEvaluationListLabel"
				                            text="{i18n>EVALUATION_TIT_STATUS}">
												<layoutData>
													<l:GridData
                                                    linebreak="true"
                                                    span="XL4 L4 M12 S12"
                                                    visibleS="false"/>
												</layoutData>
											</Label>
											<Text
				                            text="{path: 'data>STATUS_CODE', formatter: '.formatter.ideaStatus'}"/>
										</l:content>
									</l:Grid>
								</l:content>
							</l:Grid>
							<VBox>
								<Label 
				                text="{i18n>EVALUATION_FLD_EVAL_RESULT}"
				                visible="{parts: ['data>OV_RES_DATATYPE_CODE',
    				                           'data>OV_RES_NUM_VALUE',
    				                           'data>OV_RES_BOOL_VALUE',
    				                           'data>OV_RES_TEXT_VALUE',
    				                           'data>OV_RES_VALUE_OPTION_LIST_CODE',
    				                           'data>OV_RES_UOM_CODE'],
                                       formatter: '.formatter.isFormattedValueVisible'}"/>
								<Text
                                class="sapUiSmallMarginEnd sapInoEvaluationListItemOVR"
                                maxLines="2"
    				            text="{parts: ['data>OV_RES_DATATYPE_CODE',
    				                           'data>OV_RES_NUM_VALUE',
    				                           'data>OV_RES_BOOL_VALUE',
    				                           'data>OV_RES_TEXT_VALUE',
    				                           'data>OV_RES_VALUE_OPTION_LIST_CODE',
    				                           'data>OV_RES_UOM_CODE'],
                                       formatter: '.formatter.getFormattedValueEvaluations'}"
                                width="100%"></Text>
								<layoutData>
									<l:GridData span="XL1 L1 M2 S3"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label 
							class="sapInoEvaluationListLabel"
				                text="{i18n>EVALUATION_FLD_COMMENT}"
				                visible="{parts: ['data>COMMENT'],
                                       formatter: '.formatter.showEvaluationCommentDisplay'}"/>
								<Text
                                              class="sapInoEvaluationListItemComment"
                                              maxLines="1"
                                              tooltip="{path: 'data>COMMENT'}"
                                              text="{path: 'data>COMMENT'}"
                                              textAlign="Begin"
                                              width="100%"></Text>
								<layoutData>
									<l:GridData span="XL2 L2 M12 S12"/>
								</layoutData>
							</VBox>
							<CheckBox
                            class="sapInoIdeaEvaluationListSelection"
                            select="onSelectionChange"
                            visible="{object>/property/nodes/Root/customProperties/backofficeCampaignPrivilege}">
								<layoutData>
									<l:GridData span="XL1 L1 M1 S1"/>
								</layoutData>
							</CheckBox>
						</l:content>
					</l:Grid>
					<HBox class="sapInoEvalAttachment">
						<UploadCollection
                            multiple="false"
                            items="{path : 'data>EvalAttachments', sorter:[{path: 'CREATED_AT', descending: true}], templateShareable:'false'}"
                            showSeparators="true"
                            noDataText="{i18n>EMPTY_FLD_TEXT}"
                            uploadEnabled="false"
                            uploadButtonInvisible="false">
							<toolbar>
								<OverflowToolbar />
							</toolbar>
							<items>
								<UploadCollectionItem
                              		documentId="{data>ATTACHMENT_ID}"
                              		fileName="{data>FILE_NAME}"
                              		mimeType="{data>MEDIA_TYPE}"
                              		thumbnailUrl="{ parts:[{path:'data>MEDIA_TYPE'}, {path:'data>ATTACHMENT_ID'}], formatter:'.formatter.previewUrl' }"
                              		url="{ path:'data>ATTACHMENT_ID', formatter:'.formatter.downloadUrl' }"
                              		visibleEdit="false"
                              		visibleDelete="false" />
							</items>
						</UploadCollection>
						<layoutData>
							<l:GridData span="XL12 L12 M12 S12"/>
						</layoutData>
					</HBox>
				</content>
			</CustomListItem>
		</List>
		<List
            visible="{= !${object>/property/nodes/Root/customProperties/backofficeCampaignPrivilege}}"
            class="sapInoEvaluationList"
            headerText="{i18n>EVALUATION_LIST_TIT}"
            id="myEvaluationList"
            items="{path: 'data>MyEvaluations',
                    sorter: [{path: 'GROUP_ORDER',
                              descending: false,
                              group: '.grouper'},
                             {path: 'CHANGED_AT',
                              descending: true}],
                    groupHeaderFactory: '.formatter.formatGroupHeader'}"
            mode="None"
            noDataText="{i18n>EVALUATION_LBL_NO_EVALUATIONS_FOUND}">
			<headerToolbar>
				<Toolbar width="100%">
					<content>
						<Title
                            class="sapUxAPObjectPageSubSectionHeaderTitle"
                            text="{i18n>EVALUATION_OBJECT_MY}"/>
					</content>
				</Toolbar>
			</headerToolbar>
			<CustomListItem
                id="myEvaluationListItem"
                press="onItemPress"
                type="Active">
				<content>
					<l:Grid
                        class="sapUiSmallMarginTopBottom sapInoEvaluationListItemMiddle"
                        defaultSpan="XL8 L8 M8 S8"
                        hSpacing="0"
                        vSpacing="0">
						<l:content>
							<l:Grid
                            class="sapInoEvaluationListItemMiddle"
                            defaultSpan="XL8 L8 M12 S12"
                            hSpacing="0"
                            vSpacing="0">
								<l:content>
									<VBox>
										<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>CHANGED_AT', formatter: '.formatter.toDate'}"></Text>
										<Link
                                    class="sapInoEvaluationListEvaluatorName"
                                    text="{data>EVALUATOR_NAME}"
                                    press="onEvaluatorPressed"></Link>
										<layoutData>
											<l:GridData span="XL4 L4 M12 S12"/>
										</layoutData>
									</VBox>
									<l:Grid
                                    defaultSpan="XL6 L6 M12 S12"
                                    vSpacing="0"
                                    hSpacing="0">
										<l:content>
											<Label
                                            class="sapInoEvaluationListLabel"
                                            text="{i18n>EVALUATION_TIT_EVAL_METHOD}">
												<layoutData>
													<l:GridData
                                                    span="XL4 L4 M12 S12"
                                                    visibleS="false"/>
												</layoutData>
											</Label>
											<Text text="{path: 'data>MODEL_CODE', formatter: '.formatter.modelCode'}"/>
											<Label
				                            class="sapInoEvaluationListLabel"
				                            text="{i18n>EVALUATION_TIT_STATUS}">
												<layoutData>
													<l:GridData
                                                    linebreak="true"
                                                    span="XL4 L4 M12 S12"
                                                    visibleS="false"/>
												</layoutData>
											</Label>
											<Text
				                            text="{path: 'data>STATUS_CODE', formatter: '.formatter.ideaStatus'}"/>
										</l:content>
									</l:Grid>
								</l:content>
							</l:Grid>
							<Text
                            class="sapUiSmallMarginEnd sapInoEvaluationListItemOVR"
                            maxLines="2"
				            text="{parts: ['data>OV_RES_DATATYPE_CODE',
				                           'data>OV_RES_NUM_VALUE',
				                           'data>OV_RES_BOOL_VALUE',
				                           'data>OV_RES_TEXT_VALUE',
				                           'data>OV_RES_VALUE_OPTION_LIST_CODE',
				                           'data>OV_RES_UOM_CODE'],
                                   formatter: '.formatter.getFormattedValueEvaluations'}"
                            textAlign="Left"
                            width="100%">
								<layoutData>
									<l:GridData span="XL1 L1 M2 S3"/>
								</layoutData>
							</Text>
							<Text
                                              class="sapInoEvaluationListItemComment"
                                              maxLines="1"
                                              tooltip="{path: 'data>COMMENT'}"
                                              text="{path: 'data>COMMENT'}"
                                              textAlign="Begin"
                                              width="100%">
								<layoutData>
									<l:GridData span="XL2 L2 M12 S12"/>
								</layoutData>
							</Text>
							<CheckBox
                            class="sapInoIdeaEvaluationListSelection"
                            select="onSelectionChange"
                            visible="{object>/property/nodes/Root/customProperties/backofficeCampaignPrivilege}">
								<layoutData>
									<l:GridData span="XL1 L1 M1 S1"/>
								</layoutData>
							</CheckBox>
						</l:content>
					</l:Grid>
					<HBox class="sapInoEvalAttachment">
						<UploadCollection
                            multiple="false"
                            items="{path : 'data>EvalAttachments', sorter:[{path: 'CREATED_AT', descending: true}], templateShareable:'false'}"
                            showSeparators="true"
                            noDataText="{i18n>EMPTY_FLD_TEXT}"
                            uploadEnabled="false"
                            uploadButtonInvisible="false">
							<toolbar>
								<OverflowToolbar />
							</toolbar>
							<items>
								<UploadCollectionItem
                              		documentId="{data>ATTACHMENT_ID}"
                              		fileName="{data>FILE_NAME}"
                              		mimeType="{data>MEDIA_TYPE}"
                              		thumbnailUrl="{ parts:[{path:'data>MEDIA_TYPE'}, {path:'data>ATTACHMENT_ID'}], formatter:'.formatter.previewUrl' }"
                              		url="{ path:'data>ATTACHMENT_ID', formatter:'.formatter.downloadUrl' }"
                              		visibleEdit="false"
                              		visibleDelete="false" />
							</items>
						</UploadCollection>
						<layoutData>
							<l:GridData span="XL12 L12 M12 S12"/>
						</layoutData>
					</HBox>
				</content>
			</CustomListItem>
		</List>
		<List
            visible="{= !${object>/property/nodes/Root/customProperties/backofficeCampaignPrivilege}}"
            class="sapInoEvaluationList sapUiMediumMarginTop"
            headerText="{i18n>EVALUATION_LIST_TIT}"
            id="pubEvaluationList"
            items="{path: 'data>PublishedEvaluations',
                    sorter: [{path: 'GROUP_ORDER',
                              descending: false,
                              group: '.grouper'},
                             {path: 'CHANGED_AT',
                              descending: true}],
                    groupHeaderFactory: '.formatter.formatGroupHeader'}"
            mode="None"
            noDataText="{i18n>EVALUATION_LBL_NO_EVALUATIONS_FOUND}">
			<headerToolbar>
				<Toolbar width="100%">
					<content>
						<Title
                            class="sapUxAPObjectPageSubSectionHeaderTitle"
                            text="{i18n>EVALUATION_OBJECT_PUBLISHED}"/>
					</content>
				</Toolbar>
			</headerToolbar>
			<CustomListItem
                id="pubEvaluationListItem"
                press="onItemPress"
                type="Active">
				<content>
					<l:Grid
                        class="sapUiSmallMarginTopBottom sapInoEvaluationListItemMiddle"
                        defaultSpan="XL8 L8 M8 S8"
                        hSpacing="0"
                        vSpacing="0">
						<l:content>
							<l:Grid
                            class="sapInoEvaluationListItemMiddle"
                            defaultSpan="XL8 L8 M12 S12"
                            hSpacing="0"
                            vSpacing="0">
								<l:content>
									<VBox>
										<Text
                                    class="sapInoEvaluationListItemDate"
                                    text="{path: 'data>CHANGED_AT', formatter: '.formatter.toDate'}"></Text>
										<Link visible="{= ${data>EVALUATOR_ID} > 0}"
                                    class="sapInoEvaluationListEvaluatorName"
                                    text="{data>EVALUATOR_NAME}"
                                    press="onEvaluatorPressed"></Link>
										<Text visible="{= ${data>EVALUATOR_ID} === 0}"
                                    text="{i18n>IDEA_OBJECT_TIT_IDEA_Anonymity}"></Text>
										<layoutData>
											<l:GridData span="XL4 L4 M12 S12"/>
										</layoutData>
									</VBox>
									<l:Grid
                                    defaultSpan="XL6 L6 M12 S12"
                                    vSpacing="0"
                                    hSpacing="0">
										<l:content>
											<Label
                                            class="sapInoEvaluationListLabel"
                                            text="{i18n>EVALUATION_TIT_EVAL_METHOD}">
												<layoutData>
													<l:GridData
                                                    span="XL4 L4 M12 S12"
                                                    visibleS="false"/>
												</layoutData>
											</Label>
											<Text text="{path: 'data>MODEL_CODE', formatter: '.formatter.modelCode'}"/>
											<Label
				                            class="sapInoEvaluationListLabel"
				                            text="{i18n>EVALUATION_TIT_STATUS}">
												<layoutData>
													<l:GridData
                                                    linebreak="true"
                                                    span="XL4 L4 M12 S12"
                                                    visibleS="false"/>
												</layoutData>
											</Label>
											<Text
				                            text="{path: 'data>STATUS_CODE', formatter: '.formatter.ideaStatus'}"/>
										</l:content>
									</l:Grid>
								</l:content>
							</l:Grid>
							<Text
                            class="sapUiSmallMarginEnd sapInoEvaluationListItemOVR"
                            maxLines="2"
				            text="{parts: ['data>OV_RES_DATATYPE_CODE',
				                           'data>OV_RES_NUM_VALUE',
				                           'data>OV_RES_BOOL_VALUE',
				                           'data>OV_RES_TEXT_VALUE',
				                           'data>OV_RES_VALUE_OPTION_LIST_CODE',
				                           'data>OV_RES_UOM_CODE'],
                                   formatter: '.formatter.getFormattedValueEvaluations'}"
                            textAlign="Left"
                            width="100%">
								<layoutData>
									<l:GridData span="XL1 L1 M2 S3"/>
								</layoutData>
							</Text>
							<Text
                                              class="sapInoEvaluationListItemComment"
                                              maxLines="1"
                                              tooltip="{path: 'data>COMMENT'}"
                                              text="{path: 'data>COMMENT'}"
                                              textAlign="Begin"
                                              width="100%">
								<layoutData>
									<l:GridData span="XL2 L2 M12 S12"/>
								</layoutData>
							</Text>
							<CheckBox
                            class="sapInoIdeaEvaluationListSelection"
                            select="onSelectionChange"
                            visible="{object>/property/nodes/Root/customProperties/backofficeCampaignPrivilege}">
								<layoutData>
									<l:GridData span="XL1 L1 M1 S1"/>
								</layoutData>
							</CheckBox>
						</l:content>
					</l:Grid>
					<HBox class="sapInoEvalAttachment">
						<UploadCollection
                            multiple="false"
                            items="{path : 'data>EvalAttachments', sorter:[{path: 'CREATED_AT', descending: true}], templateShareable:'false'}"
                            showSeparators="true"
                            noDataText="{i18n>EMPTY_FLD_TEXT}"
                            uploadEnabled="false"
                            uploadButtonInvisible="false">
							<toolbar>
								<OverflowToolbar />
							</toolbar>
							<items>
								<UploadCollectionItem
                              		documentId="{data>ATTACHMENT_ID}"
                              		fileName="{data>FILE_NAME}"
                              		mimeType="{data>MEDIA_TYPE}"
                              		thumbnailUrl="{ parts:[{path:'data>MEDIA_TYPE'}, {path:'data>ATTACHMENT_ID'}], formatter:'.formatter.previewUrl' }"
                              		url="{ path:'data>ATTACHMENT_ID', formatter:'.formatter.downloadUrl' }"
                              		visibleEdit="false"
                              		visibleDelete="false" />
							</items>
						</UploadCollection>
						<layoutData>
							<l:GridData span="XL12 L12 M12 S12"/>
						</layoutData>
					</HBox>
				</content>
			</CustomListItem>
		</List>
	</l:Grid>
</mvc:View>