<mvc:View
    controllerName="sap.ino.vc.idea.Volunteers"
    xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:ino="sap.ino.controls"
    xmlns:l="sap.ui.layout">
    <l:Grid id="myGrid" defaultSpan="XL12 L12 M12 S12" hSpacing="0.5" vSpacing="0.5" position="Left">
        <ino:WrappingList 
            id="volunteerlist"
            class="sapInoVolunteerList"
            growing="false"
            wrapping="true"
            noDataText="{i18n>IDEA_VOLUNTEER_NO_DATA}">
        </ino:WrappingList>
    </l:Grid>
</mvc:View>