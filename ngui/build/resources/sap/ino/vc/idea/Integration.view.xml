						        tooltip="{parts:['integration>1/value','integration>1/fieldCode'],formatter:'.formatDate'}" 
						        text = "{parts:['integration>1/value','integration>1/fieldCode'],formatter:'.formatDate'}" 
						        visible="{path:'integration>1/fieldCode',formatter:'.visibleText'}"/>
					    <Link  class="sapInoIntegrationHeaderLink" tooltip="{integration>1/value}" text = "{integration>1/value}" href="{integration>1/value}" target = "_blank"
					          visible ="{path:'integration>1/fieldCode',formatter:'.visibleLink'}" enabled="{path:'integration>1/value',formatter:'.enableLink'}" />						
