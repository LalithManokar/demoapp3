<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:ino="sap.ino.controls"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <tnt:NavigationList>
        <tnt:NavigationListItem
            icon="sap-icon://decline"
            text="{i18n>CLIPBOARD_BTN_CLOSE}"
            select=".onCloseClipboard"/>
        <tnt:NavigationListItem
            icon="sap-icon://delete"
            text="{i18n>CLIPBOARD_BNT_REMOVE_ALL}"
            select=".onRemoveAllClipboardObjects"/>
        <tnt:NavigationListItem
            icon="sap-icon://sys-help"
            text="{i18n>MENU_MIT_HELP}"
            select="onNavFooterPressed"
            app:ITEM="HELP_CLIPBOARD">
        </tnt:NavigationListItem>
    </tnt:NavigationList>
</core:FragmentDefinition>
	
