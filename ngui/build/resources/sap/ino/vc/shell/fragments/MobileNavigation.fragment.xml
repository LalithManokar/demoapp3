<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog
        title="{i18n>APPLICATION_TIT_TITLE}"
        contentWidth="550px"
        contentHeight="300px"
        stretch="{= ${device>/system/phone} }">
        <content>
            <List
                items="{
                    path: '/Navigation'
                }">
                <StandardListItem
                    icon="{ICON}"
            		title="{ path : 'TITLE', formatter : '.formatter.text'}"
            		type="Active"
            		press="onNavigate"
            		visible="{ path : 'VISIBLE', formatter : '.formatter.system'}">
            		<customData>
            		    <core:CustomData
            		        key="TO"
            		        value="{TO}"/>  
            		</customData>
            	</StandardListItem>
        	</List>
        </content>
        <beginButton>
            <Button text="{i18n>MENU_BTN_CANCEL}" press="onShowMenu"></Button>
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
	
