<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<VBox visible="{= ${view>/objectExists} === false}" 
          fitContainer="true"
          alignContent="Center"
          alignItems="Center"
          justifyContent="Center">
		<Text text="{i18n>TIT_GENERAL_OBJECT_NOT_EXISTS}"
            visible="{= ${view>/insufficientObjectExists/EXISTS} !== true}" 
            textAlign="Center"
            width="100%" />
		<VBox visible="{= ${view>/insufficientObjectExists/EXISTS} === true}" >
			<Text text="{parts:[{path: 'i18n>TIT_GENERAL_OBJECT_INSUFFICIENT_ACCESS'},{path: 'view>/insufficientObjectExists/ID'}],formatter:'.formatter.formatInsufficientObjectId'}"
                textAlign="Center"
                width="100%" />
			<Link 
			    text="{parts:[{path: 'view>/insufficientObjectExists/REF_NAME'}]}" 
			    href="{parts:[{path: 'view>/insufficientObjectExists/REF_ID'}, {path: 'view>/insufficientObjectExists/OBJECT_NAME'}],formatter:'.formatter.formatInsufficientObjectExists'}" />
		</VBox>
	</VBox>
</core:FragmentDefinition>