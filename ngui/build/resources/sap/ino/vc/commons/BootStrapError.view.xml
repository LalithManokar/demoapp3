<core:View
    controllerName="sap.ino.vc.commons.BootStrapError"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:c="sap.ui.commons">

	<HBox
    	alignItems="Center"
	    justifyContent="End">
	    <Image 	class="sapInoButtonLogout sapInoImageLogout" 
	        src="/sap/ino/ngui/sap/ino/assets/img/LogOut_icon.png" 
	        press="onLogout"/>
	 <!-- -->
	</HBox>
	<VBox
	    class="sapInoWelcomeText"
	    alignItems="Center"
	    justifyContent="Center">
		<Text text="{error>/message}"/>
	</VBox>	
</core:View>

