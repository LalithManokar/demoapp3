<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ColumnListItem>
		<cells>
			<Text text="{data>CHANGED_BY_NAME}"/>
			<Text text="{parts: [{path: 'data>CHANGED_AT'}], formatter: '.formatter.toDate' }"/>
			<Text text="{parts: [{path: 'data>CHANGED_ATTRIBUTE'}], formatter: '.formatter.formatIdentityCode'}"/>
			<Text text="{parts: [{path: 'data>STRING_OLD_VALUE'},{path: 'data>INT_OLD_VALUE'},{path: 'data>DATE_OLD_VALUE'}], formatter: '.formatter.formatChangeLogValue' }"/>
			<Text text="{parts: [{path: 'data>STRING_NEW_VALUE'},{path: 'data>INT_NEW_VALUE'},{path: 'data>DATE_NEW_VALUE'}], formatter: '.formatter.formatChangeLogValue' }"/>
		</cells>
	</ColumnListItem>
</core:FragmentDefinition>