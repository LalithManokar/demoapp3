<mvc:View
    controllerName="sap.ino.vc.app.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout">
  
   <MessageStrip
        id="systemMessage"
		type="Warning"
		showIcon="true"
		showCloseButton="true">
	</MessageStrip>
   	<Breadcrumbs  id="inmBreadCrumbs">
	</Breadcrumbs>
    <Shell 
        id="innerShell"
        class="sapInoInnoMgmtMShell"
        backgroundColor="transparent"
        backgroundOpacity="0">
        <app>
            <App 
                id="app" 
                class="sapInoInnoMgmt"
                navigate=".onNavigate"
                autoFocus="false">
                <pages>
                </pages>
            </App>
        </app>
    </Shell>

    <mvc:XMLView viewName="sap.ino.vc.app.Help" id="help" visible="false"></mvc:XMLView>
</mvc:View>