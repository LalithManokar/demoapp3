<core:FragmentDefinition xmlns:core="sap.ui.core"
                         xmlns:f="sap.ui.layout.form"
                         xmlns="sap.m"
                         xmlns:ino="sap.ino.controls"
                         xmlns:mvc="sap.ui.core.mvc"
                         xmlns:form="sap.ui.layout.form"                         
                        >
                           <Panel>
				            <content > 
				            <MessageStrip class="sapUiSmallMarginBottom"
								text="{parts:['installation>/MSG_STRIP_CREATE_INPUT/message',
								               'installation>/MSG_STRIP_CREATE_INPUT/status'],
								               formatter:'.formatCreateProcessInputText'}"
								type="Error"
								visible="{parts:['installation>/MSG_STRIP_CREATE_INPUT/status'
								               ],
								               formatter:'.visibleCreateProcessInputText'}"
								showIcon="true"/>				            
    				            <Text text="{i18n>PROCESS_CREATE_INPUT_TEXT_DES}" visible="{installation>/EDIT_FORM}"/>
    				            <Text text="{i18n>PROCESS_CREATE_INPUT_SUMMARY_TEXT_DES}" visible="{=!${installation>/EDIT_FORM}}" />
                                <HBox class="sapInoInstallationCreateProcessPartMargin">
                                    <VBox width="50%">
                                        <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_TECHNIQUE_USER}" />
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER}" required="true" labelFor="processCreateSystemManager" tooltip="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER}"/>
                                            <Input id="processCreateSystemManager" value="{installation>/FORM_DATA/INNO_MANAGER_ACCOUNT}" 
                                                    editable="false"/>
                                             
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_FIRST_NAME}" required="true" labelFor="processCreateSystemManagerFirstName" tooltip="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_FIRST_NAME}"/>
                                            <Input id="processCreateSystemManagerFirstName" 
                                                   value="{
                                                          path:'installation>/FORM_DATA/INNO_MANAGER_FIRST_NAME',
                                                          type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 1,
                                            					maxLength: 15
                                            			 }                                                          
                                                        }" 
                                                    placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_FIRST_NAME}"    
                                                    editable="{installation>/EDIT_FORM}"  change="onFirstNameChange"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_LAST_NAME}" required="true" labelFor="processCreateSystemManagerLstName" tooltip="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_LAST_NAME}"/>
                                            <Input id="processCreateSystemManagerLstName"  
                                                   value="{
                                                          path:'installation>/FORM_DATA/INNO_MANAGER_LAST_NAME',
                                                          type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 1,
                                            					maxLength: 15
                                            			 }                                                            
                                                   }" 
                                                   placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_LAST_NAME}"
                                                   editable="{installation>/EDIT_FORM}" change="onLastNameChange" />
                                            
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_PASSWORD}" required="true" labelFor="processCreateSystemManagerPW" tooltip="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_PASSWORD}"/>
                                            <Input id="processCreateSystemManagerPW" 
                                                   value="{
                                                   path:'installation>/FORM_DATA/INNO_MANAGER_PASSWORD',
                                                   type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 6,
                                            					maxLength: 15
                                            			 }                                                    
                                                   }" 
                                                    editable="{installation>/EDIT_FORM}" type="Password" change="onPasswordChange" placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_PASSWORD}"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_PASSWORD_VERIFY}" required="true" labelFor="processCreateSystemManagerPWVerify" tooltip="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_PASSWORD_VERIFY}"/>
                                            <Input id="processCreateSystemManagerPWVerify"  
                                                   value="{
                                                  path:'installation>/FORM_DATA/INNO_MANAGER_REPASSWORD',
                                                   type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 6,
                                            					maxLength: 15
                                            			 }    
                                                   }" 
                                                   editable="{installation>/EDIT_FORM}" type="Password" change="onVerifyPasswordChange" placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_PASSWORD_AGAIN}" />
                                            
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_EMAIL}" required="true" labelFor="processCreateSystemManagerEmail" tooltip="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_EMAIL}"/>
                                            <Input id="processCreateSystemManagerEmail"  
                                                   value="{
                                                   path:'installation>/FORM_DATA/INNO_MANAGER_EMAIL',
                                                   type:'sap.ui.model.type.String',
                                           		   constraints: {
                                            			minLength: 1
                                            			 }                                                    
                                                   }" 
                                                   type="Email" editable="{installation>/EDIT_FORM}" change="onEmailChange" placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_EMAIL}"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER}" required="true" labelFor="processCreateTechnicalUser" tooltip="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER}"/>
                                            <Input id="processCreateTechnicalUser"  value="{installation>/FORM_DATA/TECH_USER_ACCOUNT}" 
                                                    editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER_PASSWORD}" required="true" labelFor="processCreateTechnicalUserPW" tooltip="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER_PASSWORD}"/>
                                            <Input id="processCreateTechnicalUserPW"  
                                                    value="{
                                                         path:'installation>/FORM_DATA/TECH_USER_PASSWORD',
                                                         type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 6,
                                            					maxLength: 15
                                            			 } 
                                                    }"
                                                   type="Password"
                                                   editable="{installation>/EDIT_FORM}" change="onTechUserPasswordChange" placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_PASSWORD}"/> 
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER_PASSWORD_VERIFY}" required="true" labelFor="processCreateTechnicalUserPWVerify" tooltip="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER_PASSWORD_VERIFY}"/>
                                            <Input id="processCreateTechnicalUserPWVerify"  
                                                   value="{
                                                   path:'installation>/FORM_DATA/TECH_USER_REPASSWORD',
                                                         type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 6,
                                            					maxLength: 15
                                            			 }                                                    
                                                   }"
                                                   change="onTechUserVerifyPasswordChange"
                                                   type="Password"
                                                   editable="{installation>/EDIT_FORM}" placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_PASSWORD_AGAIN}"/>                                              
                                    	</form:SimpleForm>   
                                    	<Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_EXTENSION}" />
                                   	    <form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_CUSTOM_PACKAGE_NAME}" required="true" labelFor="processCreateExtensionPackage" tooltip="{i18n>PROCESS_CREATE_INPUT_CUSTOM_PACKAGE_NAME}"/>
                                            <Input id="processCreateExtensionPackage" 
                                                   value="{
                                                   path:'installation>/FORM_DATA/PACKAGE_NAME',
                                                  type:'sap.ui.model.type.String',
                                            			  constraints : {
                                            					minLength: 1
                                            			}
                                                   }" 
                                                   placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_PACKAGE}"
                                                    editable="{installation>/EDIT_FORM}"
                                                    change="onChangePackageName"/>
                                            <Label text="" visible ="false" />
                                            <VBox align-items="center">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_PACKAGE_INFO}"
                                                   tooltip="{i18n>PROCESS_CREATE_INPUT_PACKAGE_INFO}"/>
                                            </VBox>
                                    	</form:SimpleForm>                                     	
                                    </VBox>
                                
                                    <VBox width="50%">
                                        <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_SETTINGS}" />
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SMTP_SERVER}" tooltip="{i18n>PROCESS_CREATE_INPUT_SMTP_SERVER}"/>
                                            <Input value="{installation>/FORM_DATA/SMTP_SERVER}"   editable="{installation>/EDIT_FORM}"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SMTP_SERVERPORT}" tooltip="{i18n>PROCESS_CREATE_INPUT_SMTP_SERVERPORT}" />
                                            <Input  value="{installation>/FORM_DATA/SMTP_PORT}" editable="{installation>/EDIT_FORM}"/>
                                            <Label text="" visible ="false" />
                                            <VBox align-items="center">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SET_SMTP}" tooltip="{i18n>PROCESS_CREATE_INPUT_SET_SMTP}"/>
				                            <Link  class="sapInoInstallSMTPLink" text="{parts:['i18n>PRCESS_CREATE_INPUT_SMTP_LINK_SUFFIX'],formatter:'.formatSMTPlink'}"
				                             target="_blank"
				                              href="{parts:['i18n>PRCESS_CREATE_INPUT_SMTP_LINK_SUFFIX'],formatter:'.formatSMTPlink'}" />
                                            </VBox>
                                    	</form:SimpleForm>                                        
                                        <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_APPLIATION_SETTINGS}" />
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_EMAIL_SENDER}"  tooltip="{i18n>PROCESS_CREATE_INPUT_EMAIL_SENDER}"/>
                                            <Input value="{installation>/FORM_DATA/EMAIL_SENDER}" 
                                                   placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_EMAIL}"
                                                   editable="{installation>/EDIT_FORM}" change="onEmailSenderChange"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_INTERNAL_HOST}" tooltip="{i18n>PROCESS_CREATE_INPUT_INTERNAL_HOST}" />
                                            <Input  value="{installation>/FORM_DATA/INTERNAL_HOST}" editable="{installation>/EDIT_FORM}"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_EXTERNAL_HOST}" tooltip="{i18n>PROCESS_CREATE_INPUT_EXTERNAL_HOST}" />
                                            <Input  value="{installation>/FORM_DATA/EXTERNAL_HOST}" editable="{installation>/EDIT_FORM}"/>                                            
                                    	</form:SimpleForm>                                         
                                    </VBox>                            
                                </HBox>
                                
                                 <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_PERFORMANCE_SETTINGS}" /> 
                                <HBox>
                                <VBox  width="50%">
                                    	<form:SimpleForm editable="true">
                                            <!--Label text="{i18n>PROCESS_CREATE_INPUT_ASSERTION_TIMEOUT}" /-->
                                            <!--Input value="{installation>/FORM_DATA/PERFORMANCE/TIME_OUT}" editable="{installation>/EDIT_FORM}"/-->
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_PLAN_CACHE_ENABLED}"  tooltip="{i18n>PROCESS_CREATE_INPUT_PLAN_CACHE_ENABLED}"/>
                                            <Input  value="{installation>/FORM_DATA/PERFORMANCE/RECOMPILATION_ENABLED}" 
                                                   placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_BOOLEAN}"
                                                   editable="{installation>/EDIT_FORM}"/>  
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_PLAN_CACHE_SIZE}"  tooltip="{i18n>PROCESS_CREATE_INPUT_PLAN_CACHE_SIZE}"/>
                                            <Input  value="{installation>/FORM_DATA/PERFORMANCE/PLAN_CACHE_SIZE}" 
                                                    editable="{installation>/EDIT_FORM}"
                                                    placeholder="{i18n>PROCESS_CREATE_INPUT_PLAN_CACHE_SIZE_PH}" />       
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_THRESHOLD_FACTOR}" tooltip="{i18n>PROCESS_CREATE_INPUT_THRESHOLD_FACTOR}"/>
                                            <Input value="{installation>/FORM_DATA/PERFORMANCE/THRESHOLD_FACTOR}" 
                                                   editable="{installation>/EDIT_FORM}"
                                                   placeholder="{i18n>PROCESS_CREATE_INPUT_THRESHOLD_FACTOR_PH}" />                                    	
                                    	</form:SimpleForm>                                 
                                </VBox>
                                <VBox  width="50%">
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_ENABLED}" tooltip="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_ENABLED}" />
                                            <!--Input  value="{installation>/FORM_DATA/PERFORMANCE/CACHE_ENABLE}"
                                                    placeholder="{i18n>PROCESS_CREATE_INPUT_ENTER_BOOLEAN}"
                                                    editable="{installation>/EDIT_FORM}"/-->
                                            <ComboBox items="{installation>/whiteListSection}" 
                                                      placeholder="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_ENABLED_PH}" 
                                                      selectedKey="{installation>/FORM_DATA/PERFORMANCE/CACHE_ENABLE}"
                                                      change="onComboBoxChange"
                                                      editable="{installation>/EDIT_FORM}" >
                                            <core:Item key="{installation>key}" text="{installation>text}"/>
                                           
                                            </ComboBox>        
                                                    
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_WHITE_LIST}"  tooltip="{i18n>PROCESS_CREATE_INPUT_WHITE_LIST}"/>
                                            <Input  id="createInputWhiteList"
                                                    value="{installation>/FORM_DATA/PERFORMANCE/WHITE_LIST}" 
                                                    placeholder="{i18n>PROCESS_CREATE_INPUT_WHITE_LIST_PH}"
                                                    editable="{parts:['installation>/EDIT_FORM','installation>/FORM_DATA/PERFORMANCE/CACHE_ENABLE'], formatter:'.editableWhiteList'}"/> 
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_MAX_SIZE}"  tooltip="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_MAX_SIZE}"/>
                                            <Input  value="{installation>/FORM_DATA/PERFORMANCE/CACHE_SIZE}" 
                                                     editable="{installation>/EDIT_FORM}"
                                                     placeholder="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_MAX_SIZE_PH}"  />  
                                    	</form:SimpleForm>                                 
                                </VBox>                                
                                </HBox>
	                       <Button  text="{i18n>PROCESS_CREATE_INPUT_REVIEW}" 
	                                visible="{parts:['installation>/EDIT_FORM','installation>/STEP_TO_PROCESS','installation>/USEREXISTED'],formatter:'.formatReviewButton'}" 
	                                press="onPressCreateInputPreview"/>
	                       <Button  text="{i18n>PROCESS_CREATE_INPUT_EDIT}" visible="{parts:['installation>/EDIT_FORM','installation>/STEP_TO_PROCESS','installation>/USEREXISTED'],formatter:'.formatEditButton'}" press="onPressCreateInputEdit"/>                                
			                </content>
			            </Panel>
				<Toolbar  class="sapInoInstallToolBar" visible="{parts:['installation>/STEP_TO_PROCESS'],formatter:'.visbileInputButtonToolbar'}">
				<ToolbarSpacer/>
			            <Button id="createInputBtn" text="{i18n>PROCESS_CREATE_INPUT_SUBMIT}" enabled ="{=!${installation>/EDIT_FORM}}" press="onPressCreateInputSubmit"/>
			            <Button text="{i18n>BTN_CANCEL}" press="onPressCancel"/>
		     	</Toolbar>					            
			            
</core:FragmentDefinition>