<core:FragmentDefinition xmlns:core="sap.ui.core"
                         xmlns:f="sap.ui.layout.form"
                         xmlns="sap.m"
                         xmlns:ino="sap.ino.controls"
                         xmlns:mvc="sap.ui.core.mvc"
                         xmlns:form="sap.ui.layout.form"                         
                        >
                           <Panel>
				            <content >
    				            <Text text="{i18n>PROCESS_CREATE_INPUT_SUMMARY_TEXT_DES}" />
                                <HBox class="sapInoInstallationCreateProcessPartMargin">
                                    <VBox width="50%">
                                        <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_TECHNIQUE_USER}" />
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER}" required="true" />
                                            <Input value="SYSTEM_MANAGER" editable="false" />
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_PASSWORD}" required="true" />
                                            <Input  value="SYSTEM_MANAGER" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_PASSWORD_VERIFY}" required="true" />
                                            <Input   value="SYSTEM_MANAGER" editable="false"/>
                                            
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_MANAGER_EMAIL}" required="true" />
                                            <Input   value="Email" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER}" required="true" />
                                            <Input   value="Technical User" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER_PASSWORD}" required="true" />
                                            <Input   value="TE PW" editable="false"/> 
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_TECHNICAL_USER_PASSWORD_VERIFY}" required="true" />
                                            <Input   value="TE PW" editable="false"/>                                              
                                    	</form:SimpleForm>   
                                    	<Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_EXTENSION}" />
