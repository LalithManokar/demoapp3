                                            <Label text="{i18n>PROCESS_CREATE_INPUT_CUSTOM_PACKAGE_NAME}" required="true" />
                                            <Input  value="Extension Package" editable="false"/>
                                    	</form:SimpleForm>                                     	
                                    </VBox>
                                
                                    <VBox width="50%">
                                        <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_SYSTEM_SETTINGS}" />
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SMTP_SERVER}" />
                                            <Input value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SMTP_SERVERPORT}"  />
                                            <Input  value="SMTP" editable="false"/>
                                    	</form:SimpleForm>                                        
                                        <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_APPLIATION_SETTINGS}" />
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_EMAIL_SENDER}" />
                                            <Input value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_INTERNAL_HOST}"  />
                                            <Input  value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_EXTERNAL_HOST}"  />
                                            <Input  value="SMTP" editable="false"/>                                            
                                    	</form:SimpleForm>                                         
                                    </VBox>                            
                                </HBox>
                                 <Text class = "sapInoInstallationCreateProcessSubSectionTitle" text="{i18n>PROCESS_CREATE_INPUT_PERFORMANCE_SETTINGS}" /> 
                                <HBox>
                                <VBox  width="50%">
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_ASSERTION_TIMEOUT}" />
                                            <Input value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_SERVER_CACHE}"  />
                                            <Input  value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_MAX_ENTRIES}"  />
                                            <Input  value="SMTP" editable="false"/> 
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_PLAN_CACHE_ENABLED}"  />
