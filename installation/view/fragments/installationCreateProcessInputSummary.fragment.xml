                                </VBox>
                                <VBox  width="50%">
                                    	<form:SimpleForm editable="true">
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_THRESHOLD_FACTOR}" />
                                            <Input value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_ENABLED}"  />
                                            <Input  value="SMTP" editable="false"/>
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_WHITE_LIST}"  />
                                            <Input  value="SMTP" editable="false"/> 
                                            <Label text="{i18n>PROCESS_CREATE_INPUT_RESULT_CACHE_MAX_SIZE}"  />
                                            <Input  value="SMTP" editable="false"/>  
                                    	</form:SimpleForm>                                 
                                </VBox>                                
                                </HBox>
			                </content>
			            </Panel>
	
				<OverflowToolbar id ="createInputSummaryToolbar">
				<ToolbarSpacer/>
			            <Button id="createInputSummaryBtn" text="{i18n>PROCESS_CREATE_INPUT_SUMMARY_NEXT}" press="onPressCreateInputSummaryNextBtn"/>
			            <Button text="{i18n>BTN_EDIT}" press="onPressBacktoEdit"/>
			            <Button text="{i18n>BTN_CANCEL}" press="onPressCancel"/>
			</OverflowToolbar>				            
			            
</core:FragmentDefinition>