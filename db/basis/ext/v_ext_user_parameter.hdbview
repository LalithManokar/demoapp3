schema = "SAP_INO_EXT";
query = "select param.* 
         from 
             \"sap.ino.db.iam::v_auth_application_user\" as user
         inner join
             \"sap.ino.db.basis::v_user_parameter\" as param
         on
             user.id = param.user_id
         with read only";

depends_on_view = ["sap.ino.db.iam::v_auth_application_user",
                   "sap.ino.db.basis::v_user_parameter"];