schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.campaign::t_campaign\" 
         union all 
         select max(id) as id from \"sap.ino.db.campaign::t_campaign_h\" )";

depends_on_table = ["sap.ino.db.campaign::t_campaign",
                    "sap.ino.db.campaign::t_campaign_h"];