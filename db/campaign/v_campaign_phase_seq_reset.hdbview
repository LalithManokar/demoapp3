schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from
        (select max(id) as id from \"sap.ino.db.campaign::t_campaign_phase\"
         union all
         select max(id) as id from \"sap.ino.db.campaign::t_campaign_phase_h\" )";
depends_on_table = ["sap.ino.db.campaign::t_campaign_phase",
                    "sap.ino.db.campaign::t_campaign_phase_h"]; 