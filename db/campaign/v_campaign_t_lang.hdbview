schema = "SAP_INO";
query = "
    select text.*, language.default_text as lang_text
    from \"sap.ino.db.campaign::t_campaign_t\" as text 
    left outer join \"sap.ino.db.basis::v_languages\" as language
        on language.iso_code = text.lang
    with read only";

depends_on_table = ["sap.ino.db.campaign::t_campaign_t"];
depends_on_view  = ["sap.ino.db.basis::v_languages"];