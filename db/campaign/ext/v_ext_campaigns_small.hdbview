schema = "SAP_INO_EXT";
query = "select campaign.* from \"sap.ino.db.campaign::v_campaign_small\" as campaign
            join \"sap.ino.db.campaign::v_auth_campaigns\" as auth
            on campaign.id = auth.campaign_id with read only";
depends_on_view = ["sap.ino.db.campaign::v_campaign_small", "sap.ino.db.campaign::v_auth_campaigns"];
  