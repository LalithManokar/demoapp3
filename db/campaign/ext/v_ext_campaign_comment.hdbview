schema = "SAP_INO_EXT";
query = "select campaign_comment.*
         from \"sap.ino.db.campaign::v_campaign_comment\" as campaign_comment
         inner join \"sap.ino.db.campaign::v_auth_campaigns\" as auth_campaigns
         on auth_campaigns.campaign_id = campaign_comment.object_id
         with read only";
depends_on_view = ["sap.ino.db.campaign::v_campaign_comment", "sap.ino.db.campaign::v_auth_campaigns"];