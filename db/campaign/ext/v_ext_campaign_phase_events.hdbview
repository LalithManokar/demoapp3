schema = "SAP_INO_EXT";

query = "select campaign_phase.* from \"sap.ino.db.campaign::v_campaign_process_events_filtered\" as campaign_phase
            join \"sap.ino.db.campaign::v_auth_campaigns\" as auth
            on campaign_phase.campaign_id = auth.campaign_id";

depends_on_view = ["sap.ino.db.campaign::v_auth_campaigns",
                   "sap.ino.db.campaign::v_campaign_process_events_filtered"];
