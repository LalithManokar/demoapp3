schema = "SAP_INO_EXT";
query = "select campaign.id as id, campaign.name as name from \"sap.ino.db.campaign::v_campaign_full\" as campaign
            join \"sap.ino.db.campaign::v_auth_campaigns\" as auth
            on campaign.id = auth.campaign_id with read only";
depends_on_view = ["sap.ino.db.campaign::v_campaign_full", "sap.ino.db.campaign::v_auth_campaigns"];
 