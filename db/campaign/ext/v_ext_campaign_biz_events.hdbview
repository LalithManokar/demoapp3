schema = "SAP_INO_EXT";
query = "select
             campaign_events.*
         from \"sap.ino.db.campaign::v_campaign_biz_events\" as campaign_events
             inner join \"sap.ino.db.campaign::v_auth_campaigns\" as auth_campaigns
               on auth_campaigns.campaign_id = campaign_events.campaign_id
with read only";

depends_on_view = [ "sap.ino.db.campaign::v_campaign_biz_events", 
                    "sap.ino.db.campaign::v_auth_campaigns"];