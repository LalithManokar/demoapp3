schema = "SAP_INO_EXT";
query = "
    select  data.* from 
        \"sap.ino.db.analytics::v_analytics_static_top_commentators_campaign\" as data
    inner join
        \"sap.ino.db.campaign::v_auth_campaigns\" as auth
        on data.campaign_id = auth.campaign_id
    with read only";

depends_on_view = ["sap.ino.db.analytics::v_analytics_static_top_commentators_campaign",
                   "sap.ino.db.campaign::v_auth_campaigns"];