schema = "SAP_INO_EXT";

query = "select campaign.* from \"sap.ino.db.campaign::v_campaign_page_t_lang\" as campaign
            left outer join \"sap.ino.db.campaign::v_auth_campaigns\" as auth
            on campaign.campaign_id = auth.campaign_id";

depends_on_view = ["sap.ino.db.campaign::v_campaign_page_t_lang", 
                   "sap.ino.db.campaign::v_auth_campaigns"];
