schema = "SAP_INO";
query = "
select
    campaign_phase_history.*
from
    \"sap.ino.db.campaign::v_campaign_process_events_pre\" as campaign_phase_history
where
    campaign_phase_history.history_db_event<>'DELETED'
with read only";

depends_on_view = ["sap.ino.db.campaign::v_campaign_process_events_pre"];