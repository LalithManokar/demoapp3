schema = "SAP_INO";
query  = "
          select reward.id as reward_id
             from \"sap.ino.db.reward::v_reward\" as reward
             where status_code = 'sap.ino.config.REWARD_PUB_AUTHOR' or
                   status_code = 'sap.ino.config.REWARD_PUB_COMMUNITY'
          with read only";
depends_on_view = ["sap.ino.db.reward::v_reward"];          