schema = "SAP_INO_EXT"; 
query  = " select reward.* from \"sap.ino.db.reward::v_reward\" as reward 
            where reward.reward_list_id in (select reward_list_id from \"sap.ino.db.reward::v_auth_rewards_read\")
            with read only";
depends_on_view = ["sap.ino.db.reward::v_reward",
                    "sap.ino.db.reward::v_auth_rewards_read"];