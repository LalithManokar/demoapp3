      <viewAttributes>
        <viewAttribute datatype="DOUBLE" id="SCORE"/>
        <viewAttribute datatype="INTEGER" id="EMPLOYEE_ID"/>
        <viewAttribute datatype="NVARCHAR" id="EMPLOYEE_NAME" length="100"/>
        <viewAttribute datatype="NVARCHAR" id="EMPLOYEE_EMAIL" length="256"/>
        <viewAttribute datatype="NVARCHAR" id="USER_NAME" length="256"/>
