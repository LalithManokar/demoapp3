        <viewAttribute datatype="DOUBLE" id="REWARD_AMOUNT"/>
        <viewAttribute datatype="NVARCHAR" id="REWARD_UNIT" length="100"/>
        <viewAttribute datatype="NVARCHAR" id="REWARD_UNIT_DESCRIPTION" length="300"/>
        <viewAttribute datatype="DOUBLE" id="REWARD_SHARE"/>
        <viewAttribute datatype="INTEGER" id="DOWNLOAD_COUNT"/>
