        <viewAttribute datatype="DATE" id="CREATED_AT"/>
        <viewAttribute datatype="INTEGER" id="GROUP_ORDER"/>
      </viewAttributes>
      <calculatedViewAttributes/>
      <localVariable>#filterName</localVariable>
      <localVariable>#searchToken</localVariable>
      <definition> 
 /********* Begin Procedure Script ************/ 
 BEGIN 
    
    -- call &quot;SAP_INO&quot;.&quot;sap.ino.db.reward::p_reward_search&quot;(:searchToken, :lt_search_rewards);
    lt_search_rewards = select id, 1 as score 
        from
            &quot;sap.ino.db.reward::t_reward&quot;
        where
            length(:searchToken) = 0 or
            cast(id as nvarchar) = :searchToken;
    lt_filter_rewards = select reward.id as reward_id
                          from &quot;sap.ino.db.reward::t_reward&quot; as reward
                          where :filterName &lt;&gt; 'submittedReward'        and
                                :filterName &lt;&gt; 'publishedReward'        and
                                :filterName &lt;&gt; 'rewardManage'           and
                                :filterName &lt;&gt; 'readyForDownloadReward' and
                                :filterName &lt;&gt; 'downloadedReward'       and
                                reward.reward_list_id in (select reward_list_id from &quot;sap.ino.db.reward::v_auth_rewards_read&quot;)
                        union all
                        select reward.reward_id
                          from &quot;sap.ino.db.reward::v_submitted_reward&quot; as reward
                          where :filterName = 'submittedReward'
                        union all 
                        select reward.reward_id
                          from &quot;sap.ino.db.reward::v_published_reward&quot; as reward
                          where :filterName = 'publishedReward'
                        union all
                        select reward.reward_id
                          from &quot;sap.ino.db.reward::v_reward_ready_for_download&quot; as reward
                          inner join &quot;sap.ino.db.reward::v_auth_rewards_manage&quot; as auth_manage
                            on reward.reward_list_id = auth_manage.reward_list_id
                          where :filterName = 'readyForDownloadReward'
                        union all
                        select reward.reward_id 
                          from &quot;sap.ino.db.reward::v_downloaded_reward&quot; as reward
                          inner join &quot;sap.ino.db.reward::v_auth_rewards_manage&quot; as auth_manage
                            on reward.reward_list_id = auth_manage.reward_list_id
                          where :filterName = 'downloadedReward'
                        union all
                        select reward.id as reward_id
                          from &quot;sap.ino.db.reward::v_reward&quot; as reward
                          inner join &quot;sap.ino.db.reward::v_auth_rewards_manage&quot; as auth_manage
                            on reward.reward_list_id = auth_manage.reward_list_id
                          where :filterName = 'rewardManage'; 
                          
 	 var_out = select lt_search_rewards.score as score, reward.*
 	             from :lt_filter_rewards            as lt_filter_rewards,
 	                  &quot;sap.ino.db.reward.ext::v_ext_reward&quot; as reward
 	             inner join :lt_search_rewards as lt_search_rewards
 	               on lt_search_rewards.id = reward.id
 	             where lt_filter_rewards.reward_id = reward.id;


END /********* End Procedure Script ************/</definition>
    </calculationView>
  </calculationViews>
  <logicalModel id="Script_View">
