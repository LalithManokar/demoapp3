schema = "SAP_INO_EXT";
query = "select idea_submitter.* 
            from \"sap.ino.db.idea::v_idea_submitter\" as idea_submitter 
			where idea_submitter.idea_id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
            with read only"; 

depends_on_view = ["sap.ino.db.idea::v_idea_submitter","sap.ino.db.idea::v_auth_ideas_read"];