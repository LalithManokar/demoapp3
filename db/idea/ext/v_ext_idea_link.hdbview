schema = "SAP_INO_EXT";
query = "select idea_links.* 
            from \"sap.ino.db.idea::v_idea_link\" as idea_links
            where idea_links.idea_id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
            with read only";
 
depends_on_view = ["sap.ino.db.idea::v_idea_link",
				   "sap.ino.db.idea::v_auth_ideas_read"];