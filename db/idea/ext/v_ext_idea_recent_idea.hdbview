schema = "SAP_INO_EXT";
query = "select idea.*
         from \"sap.ino.db.idea::v_idea_recent_idea\" as idea
         where idea.id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
         with read only";

depends_on_view = ["sap.ino.db.idea::v_idea_recent_idea",
                   "sap.ino.db.idea::v_auth_ideas_read"];