schema = "SAP_INO_EXT";
query = "select idea_phase.*
         from \"sap.ino.db.idea::v_idea_phase\" as idea_phase
         where idea_phase.idea_id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
         with read only";

depends_on_view = [ "sap.ino.db.idea::v_idea_phase", 
                    "sap.ino.db.idea::v_auth_ideas_read"];