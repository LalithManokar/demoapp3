schema = "SAP_INO_EXT";
query = "select all_stakeholders.*
            from \"sap.ino.db.idea::v_idea_all_stakeholders\" as all_stakeholders
            where all_stakeholders.idea_id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
            with read only";

depends_on_view = ["sap.ino.db.idea::v_idea_all_stakeholders", "sap.ino.db.idea::v_auth_ideas_read"];