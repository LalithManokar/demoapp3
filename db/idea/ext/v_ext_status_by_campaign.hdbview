schema = "SAP_INO_EXT";

query = "select campaign_status.* from \"sap.ino.db.idea::v_status_by_campaign\" as campaign_status
            inner join \"sap.ino.db.campaign::v_auth_campaigns\" as auth
            on campaign_status.campaign_id = auth.campaign_id";

depends_on_view = ["sap.ino.db.campaign::v_auth_campaigns", "sap.ino.db.idea::v_status_by_campaign"];
