schema = "SAP_INO_EXT";
query  = "
          select idea.* from \"sap.ino.db.idea::v_qualified_idea_for_reward\" as idea
                  where exists( select * from \"sap.ino.db.idea::v_auth_qualified_ideas_read\" as auth_campaign where auth_campaign.campaign_id = idea.campaign_id )
          with read only";
depends_on_view = ["sap.ino.db.idea::v_qualified_idea_for_reward",
                   "sap.ino.db.idea::v_auth_qualified_ideas_read"];