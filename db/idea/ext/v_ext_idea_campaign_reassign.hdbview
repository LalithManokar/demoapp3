schema = "SAP_INO_EXT";
query = "select distinct campaign_idea.* from \"sap.ino.db.idea::v_idea_campaign_reassign\" as campaign_idea
            inner join \"sap.ino.db.idea::v_auth_ideas_update\" as idea_auth
            on idea_auth.idea_id = campaign_idea.idea_id
			with read only";

depends_on_view =  ["sap.ino.db.idea::v_idea_campaign_reassign",
                    "sap.ino.db.idea::v_auth_ideas_update"];
