schema = "SAP_INO_EXT";
query = "select idea_author.* 
            from \"sap.ino.db.idea::v_idea_lean_author\" as idea_author 
            where idea_author.idea_id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
            with read only";

depends_on_view = ["sap.ino.db.idea::v_idea_lean_author","sap.ino.db.idea::v_auth_ideas_read"];