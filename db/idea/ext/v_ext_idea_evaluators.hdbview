schema = "SAP_INO_EXT";
query = "select evaluators.*
            from \"sap.ino.db.idea::v_idea_lean_evaluators\" as evaluators
            where evaluators.idea_id in (select idea_id from \"sap.ino.db.idea::v_auth_ideas_read\")
            with read only";

depends_on_view = ["sap.ino.db.idea::v_idea_lean_evaluators", "sap.ino.db.idea::v_auth_ideas_read"];