// get idea ids which the current login user has voted 

schema = "SAP_INO";
query = "
		select 
			distinct idea_id 
		from \"sap.ino.db.idea::t_vote\" 
		where user_id in (select id from \"sap.ino.db.iam::v_auth_application_user\")
		with read only
";

depends_on_table=["sap.ino.db.idea::t_vote"];  
depends_on_view=["sap.ino.db.iam::v_auth_application_user"];
