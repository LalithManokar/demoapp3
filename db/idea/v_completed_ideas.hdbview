schema = "SAP_INO";
query = "select distinct idea.id as id
	from \"sap.ino.db.idea::t_idea\" as idea
	where
	   status_code in (select * from \"sap.ino.db.idea::v_status_code_for_completed_idea\")
	with read only";

depends_on_table = ["sap.ino.db.idea::t_idea"];
depends_on_view = ["sap.ino.db.idea::v_status_code_for_completed_idea"];