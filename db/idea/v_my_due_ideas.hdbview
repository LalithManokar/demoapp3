schema = "SAP_INO";
query = "select distinct follow.object_id as id, idea.campaign_id 
            from \"sap.ino.db.followup::v_follow_up\" as follow
            inner join \"sap.ino.db.idea::t_idea\" as idea 
                on follow.object_id = idea.id and follow.object_type_code = 'IDEA' and follow.date <= current_date
                and idea.status_code <> 'sap.ino.config.DRAFT'";

depends_on_table = ["sap.ino.db.idea::t_idea"];
depends_on_view = ["sap.ino.db.followup::v_follow_up"];