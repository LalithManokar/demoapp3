schema = "SAP_INO";
query = "select idea.id, idea.campaign_id, idea.status_code
	from \"sap.ino.db.idea::t_idea\" as idea 
	where id not in 
		(select object_id from \"sap.ino.db.iam::t_object_identity_role\" 
			where object_type_code = 'IDEA' and 
                  role_code = 'IDEA_COACH') with read only";

depends_on_table = ["sap.ino.db.idea::t_idea", 
					"sap.ino.db.iam::t_object_identity_role"];