schema = "SAP_INO";
query = "select idea_tag.object_id as idea_id, idea_tag.id, idea_tag.tag_id, tag.name 
    from \"sap.ino.db.tag::t_object_tag\" as idea_tag 
       inner join \"sap.ino.db.tag::t_tag\" as tag
        on idea_tag.tag_id = tag.id and idea_tag.object_type_code = 'IDEA'";

depends_on_table = ["sap.ino.db.tag::t_tag",
                    "sap.ino.db.tag::t_object_tag"];
