schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.idea::t_idea\" 
         union all 
         select max(id) as id from \"sap.ino.db.idea::t_idea_h\" )";

depends_on_table = ["sap.ino.db.idea::t_idea",
                    "sap.ino.db.idea::t_idea_h"];