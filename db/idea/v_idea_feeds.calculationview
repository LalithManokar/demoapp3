        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="FEED_CODE" length="600" expressionLanguage="COLUMN_ENGINE">
          <formula>case(&quot;ROLE_CODE&quot;,
'IDEA_COACH',&quot;HISTORY_BIZ_EVENT&quot;,
'IDEA_CONTRIBUTOR',case(&quot;HISTORY_DB_EVENT&quot;, 'CREATED','CONTRIB_ADDED','DELETED','CONTRIB_REMOVED'),
'IDEA_EXPERT',case(&quot;HISTORY_DB_EVENT&quot;, 'CREATED','EXPERT_ASSIGNED','DELETED','EXPERT_UNASSIGNED'),
' ')</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="#sap.ino.db.iam::t_object_identity_role_h">
        <mapping xsi:type="Calculation:AttributeMapping" target="EVENT_AT" source="HISTORY_AT"/>
