        <mapping xsi:type="Calculation:AttributeMapping" target="HISTORY_DB_EVENT" source="HISTORY_DB_EVENT"/>
      </input>
      <filter>&quot;OBJECT_TYPE_CODE&quot; = 'IDEA' and 
&quot;HISTORY_DB_EVENT&quot; != 'UPDATED' and
(( &quot;ROLE_CODE&quot; = 'IDEA_COACH'  and 
(&quot;HISTORY_BIZ_EVENT&quot; = 'COACH_ASSIGNED' or &quot;HISTORY_BIZ_EVENT&quot; = 'COACH_UNASSIGNED')) or 
&quot;ROLE_CODE&quot; = 'IDEA_CONTRIBUTOR'  or 
&quot;ROLE_CODE&quot; = 'IDEA_EXPERT' )</filter>
    </calculationView>
