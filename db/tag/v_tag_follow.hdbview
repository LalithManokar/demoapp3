schema = "SAP_INO";
query = "select
            tag_follow.*
        from 
            \"sap.ino.db.follow::v_follow\" as tag_follow
        inner join 
            \"sap.ino.db.iam::v_auth_application_user\" as user
        on
            tag_follow.OBJECT_TYPE_CODE = 'TAG'
        and
            tag_follow.CREATED_BY_ID = user.id
        with read only"; 

depends_on_view  = ["sap.ino.db.follow::v_follow", "sap.ino.db.iam::v_auth_application_user"];