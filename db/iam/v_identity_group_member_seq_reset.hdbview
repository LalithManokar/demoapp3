schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.iam::t_identity_group_member\" 
         union all 
         select max(id) as id from \"sap.ino.db.iam::t_identity_group_member_h\" )";

depends_on_table = ["sap.ino.db.iam::t_identity_group_member",
                    "sap.ino.db.iam::t_identity_group_member_h"];