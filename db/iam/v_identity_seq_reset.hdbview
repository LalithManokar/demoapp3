schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.iam::t_identity\" 
         union all 
         select max(id) as id from \"sap.ino.db.iam::t_identity_h\" )";

depends_on_table = ["sap.ino.db.iam::t_identity",
                    "sap.ino.db.iam::t_identity_h"];