schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.iam::t_object_identity_role\" 
         union all 
         select max(id) as id from \"sap.ino.db.iam::t_object_identity_role_h\" )";

depends_on_table = ["sap.ino.db.iam::t_object_identity_role",
                    "sap.ino.db.iam::t_object_identity_role_h"];