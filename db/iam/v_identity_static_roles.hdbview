schema = "SAP_INO";
query = "
    select
        row_number() over () as id,
        identity.id as identity_id,
        static_roles.logical_role_name,
        static_roles.qualified_role_name
    from \"sap.ino.db.iam::t_identity\" as identity 
        inner join \"sap.ino.db.iam::v_sys_granted_roles\" as identity_roles 
            on identity_roles.grantee = identity.user_name
        inner join \"sap.ino.db.iam::v_static_roles\" as static_roles 
            on identity_roles.role_name = static_roles.qualified_role_name
    with read only";

depends_on_table = ["sap.ino.db.iam::t_identity"];
depends_on_view = ["sap.ino.db.iam::v_static_roles", "sap.ino.db.iam::v_sys_granted_roles"];