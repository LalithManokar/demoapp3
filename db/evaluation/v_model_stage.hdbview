schema = "SAP_INO";
query = "
    select  
        model.*,
        created_by_identity.name as created_by, 
        changed_by_identity.name as changed_by 
    from \"sap.ino.db.evaluation::t_model_stage\" as model
        left outer join \"sap.ino.db.iam::t_identity\" as created_by_identity
            on model.created_by_id = created_by_identity.id
        left outer join \"sap.ino.db.iam::t_identity\" as changed_by_identity
            on model.changed_by_id = changed_by_identity.id
    with read only";

depends_on_table = ["sap.ino.db.evaluation::t_model_stage",
                    "sap.ino.db.iam::t_identity"];