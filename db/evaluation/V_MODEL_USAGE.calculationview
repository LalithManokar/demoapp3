      <definition> 
 /********* Begin Procedure Script ************/ 
 BEGIN 
     var_out = 
        select 
          model_usage.model_code,
          model_usage.campaign_id,
          model_usage.campaign_name,
          model_usage.campaign_status_code,
          model_usage.idea_id,
          model_usage.idea_name,
          model_usage.evaluation_count
        from &quot;sap.ino.db.evaluation::v_model_usage&quot; 
        as model_usage; 

END /********* End Procedure Script ************/</definition>
    </calculationView>
  </calculationViews>
