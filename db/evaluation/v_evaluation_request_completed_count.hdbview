schema = "SAP_INO";
query  = "
         select 
           eval_req_id, 
           COMPLETED_ON_TIME, 
           count(*) as eval_req_completed_count 
         from \"sap.ino.db.evaluation::v_evaluation_request_full_item\" 
         where completed_on_time is not null 
         group by eval_req_id, COMPLETED_ON_TIME order by eval_req_id
    with read only";
    
depends_on_view = ["sap.ino.db.evaluation::v_evaluation_request_full_item"];    