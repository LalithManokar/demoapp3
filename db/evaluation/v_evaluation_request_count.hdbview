schema = "SAP_INO";
query  ="
         select 
            eval_req_id, 
            count(*)  as eval_req_count 
          from 
            \"sap.ino.db.evaluation::v_evaluation_request_full_item\"
          group by eval_req_id
    with read only";
    
depends_on_view = ["sap.ino.db.evaluation::v_evaluation_request_full_item"];    