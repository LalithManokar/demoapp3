schema = "SAP_INO";
query = "
    select * from \"sap.ino.db.evaluation::v_evaluation_request_item\"
        where (status_code = 'sap.ino.config.EVAL_REQ_REQUESTED' 
                and add_days(accept_date,1) < CURRENT_UTCTIMESTAMP) or
        ((status_code = 'sap.ino.config.EVAL_REQ_ACCEPTED' or
          status_code = 'sap.ino.config.EVAL_REQ_IN_PROCESS' or
          status_code = 'sap.ino.config.EVAL_REQ_IN_CLARIFICATION') and 
          add_days(complete_date,1) < CURRENT_UTCTIMESTAMP)
    with read only
";

depends_on_view = ["sap.ino.db.evaluation::v_evaluation_request_item"];