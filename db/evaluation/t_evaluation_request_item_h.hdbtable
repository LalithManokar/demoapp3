table.schemaName = "SAP_INO";
table.tableType = COLUMNSTORE;
table.description = "Evaluation Request";
table.columns = [
    {name = "HISTORY_DB_EVENT";  sqlType = VARCHAR;   nullable = false; length = 8;    comment = "Database event: CREATED, UPDATED, DELETED";},
    {name = "HISTORY_BIZ_EVENT"; sqlType = VARCHAR;   nullable = false; length = 600;  comment = "Business event: EVAL_REQ_CREATED, EVAL_REQ_UPDATED ...";},
    {name = "HISTORY_AT";        sqlType = TIMESTAMP; nullable = false;                comment = "Timestamp when event occurred";},
    {name = "HISTORY_ACTOR_ID";  sqlType = INTEGER;   nullable = false;                comment = "Actor who did the changes";},
    {name = "ID";                sqlType = INTEGER;   nullable = false;                comment = "Human readable identifier";},
    {name = "EVAL_REQ_ID";       sqlType = INTEGER;   nullable = false;                comment = "Evaluation Request ID";},
    {name = "STATUS_CODE";       sqlType = VARCHAR;   nullable = false; length = 500;  comment = "Status Code";},
    {name = "COMMENT_FORWARD";   sqlType = NVARCHAR;  nullable = true;  length = 1024; comment = "comment when forward"; },
    {name = "CREATED_AT";        sqlType = TIMESTAMP; nullable = true;                comment = "Creation timestamp";},
    {name = "CREATED_BY_ID";     sqlType = INTEGER;   nullable = true;                comment = "Identity by which an identity has been created. Foreign key to t_identity.ID";},
    {name = "CHANGED_AT";        sqlType = TIMESTAMP; nullable = true;                comment = "Changed timestamp";},
    {name = "CHANGED_BY_ID";     sqlType = INTEGER;   nullable = true;                comment = "Identity by which an identity has been changed. Foreign key t_identity.ID";}
];
table.indexes = [
    {name="LOGKEY_EVALUTION_REQ_ITEM_H"; unique = false; indexColumns = ["ID","HISTORY_AT"];}
];