schema = "SAP_INO";
query = "
select 
    evaluation.id as evaluation_id,
    campaign_phase.*
from \"sap.ino.db.evaluation::t_evaluation\" as evaluation 
    inner join \"sap.ino.db.idea::t_idea\" as idea
        on idea.id = evaluation.idea_id 
    inner join \"sap.ino.db.campaign::t_campaign_phase\" as campaign_phase
    	on idea.campaign_id = campaign_phase.campaign_id
";

depends_on_table =  ["sap.ino.db.evaluation::t_evaluation",
					 "sap.ino.db.idea::t_idea",
                     "sap.ino.db.campaign::t_campaign_phase"];