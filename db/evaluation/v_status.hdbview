schema = "SAP_INO";
query = "
-- Filter evaluation status codes so that only those are shown which are used in the 
-- model sap.ino.config.EVALUATION
select distinct code from 
(
    select current_status_code as code, status_model_code
    from \"sap.ino.db.status::t_status_model_transition\"
    union all 
    select next_status_code as code, status_model_code
    from \"sap.ino.db.status::t_status_model_transition\"
)
where status_model_code = \'sap.ino.config.EVALUATION\'
";
depends_on_table = ["sap.ino.db.status::t_status_model_transition"];