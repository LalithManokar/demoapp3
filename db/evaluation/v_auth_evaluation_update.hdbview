schema = "SAP_INO";
query = 
"
select object_id as evaluation_id, role_eval.identity_id 
from \"sap.ino.db.iam::v_auth_application_user_role_transitive\" as role_eval
inner join \"sap.ino.db.iam::t_role_privilege\" as role_privilege
    on  role_privilege.role_code = role_eval.role_code 
where   role_eval.object_type_code = 'EVALUATION' and 
        role_privilege.privilege = 'EVALUATION_UPDATE'
with read only
";

depends_on_table = ["sap.ino.db.iam::t_role_privilege"];

depends_on_view = ["sap.ino.db.iam::v_auth_application_user_role_transitive"];