schema = "SAP_INO_EXT";
query = "
select eval_req_item_forward.*
from \"sap.ino.db.evaluation::v_evaluation_request_item_forward\" as eval_req_item_forward
where eval_req_item_forward.eval_req_item_id in (select eval_req_item_id from \"sap.ino.db.evaluation::v_auth_evaluation_request_item_read\" )
with read only ";

