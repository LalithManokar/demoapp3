schema = "SAP_INO";
query = "select object_role.object_id as attachment_id, object_role.identity_id from \"sap.ino.db.iam::v_auth_application_user_role_transitive\" as object_role
inner join \"sap.ino.db.iam::t_role_privilege\" as role 
	on role.role_code = object_role.role_code and object_role.object_type_code = 'ATTACHMENT'
	and role.privilege = 'ATTACHMENT_UPDATE'
		with read only";

depends_on_table = ["sap.ino.db.iam::t_role_privilege"];
depends_on_view = ["sap.ino.db.iam::v_auth_application_user_role_transitive"];
