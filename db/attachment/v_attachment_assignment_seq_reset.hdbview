schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.attachment::t_attachment_assignment\" 
         union all 
         select max(id) as id from \"sap.ino.db.attachment::t_attachment_assignment_h\" )";

depends_on_table = ["sap.ino.db.attachment::t_attachment_assignment",
                    "sap.ino.db.attachment::t_attachment_assignment_h"];
