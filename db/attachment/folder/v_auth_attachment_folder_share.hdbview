schema = "SAP_INO";
query = "select auth.folder_id from \"sap.ino.db.attachment.folder::v_auth_attachment_folder_share_read\" as auth
	inner join \"sap.ino.db.iam::v_auth_application_user\" as suser
    	on auth.identity_id = suser.id
	with read only";

depends_on_view = ["sap.ino.db.attachment.folder::v_auth_attachment_folder_share_read",
				   "sap.ino.db.iam::v_auth_application_user"];
