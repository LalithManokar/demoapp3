schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.comment::t_comment\" 
         union all 
         select max(id) as id from \"sap.ino.db.comment::t_comment_h\" )";

depends_on_table = ["sap.ino.db.comment::t_comment",
                    "sap.ino.db.comment::t_comment_h"];