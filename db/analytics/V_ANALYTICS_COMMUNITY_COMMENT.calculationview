    <calculatedViewAttributes/>
    <definition>begin
    var_out = select id,
                     created_at,
                     created_by_id,
                     object_id,
                     object_type_code
                 from &quot;sap.ino.db.comment::v_community_comment&quot;;
end </definition>
  </calculationView>
</calculationViews>
