      <viewAttribute id="CREATED_BY_ID" datatype="INTEGER"/>
      <viewAttribute id="OBJECT_ID" datatype="INTEGER"/>
      <viewAttribute id="OBJECT_TYPE_CODE" datatype="VARCHAR" length="100"/>
