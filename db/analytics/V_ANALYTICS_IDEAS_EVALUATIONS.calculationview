 	 var_out = 
 	   select 
 	      idea_id,
 	      campaign_id,
 	      campaign_phase_code,
 	      model_code,
 	      evaluation_id,
 	      evaluation_created_at,
 	      evaluation_created_by_id,
 	      evaluation_changed_at,
 	      evaluation_changed_by_id,
 	      evaluation_status_code,
