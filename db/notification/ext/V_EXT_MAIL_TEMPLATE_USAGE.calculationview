 /********* Begin Procedure Script ************/ 
 BEGIN 
     var_out = 
        select 
          template_usage.mail_template_code,
          template_usage.campaign_id,
          template_usage.campaign_name,
          template_usage.campaign_status_code,
          template_usage.campaign_count
        from &quot;sap.ino.db.notification::v_mail_template_usage&quot; 
        as template_usage; 

END /********* End Procedure Script ************/</definition>
    </calculationView>
