procedure "SAP_INO"."sap.ino.db.notification::p_notification_email_status_update" (
    in  it_notification_status_ids "SAP_INO"."sap.ino.db.notification::tt_notification_id",
    in  iv_status varchar(16)
)
    language sqlscript 
    sql security definer
    default schema SAP_INO
    as
begin
    update "sap.ino.db.notification::t_notification_status" 
        set mail_status_code = :iv_status
        where notification_id in (select id from :it_notification_status_ids);
end;
