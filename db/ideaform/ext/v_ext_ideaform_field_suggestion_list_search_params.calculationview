      </attribute>
      <attribute id="DATATYPE_CODE" order="2">
        <descriptions defaultDescription="DATATYPE_CODE"/>
        <keyMapping columnObjectName="Script_View" columnName="DATATYPE_CODE"/>
      </attribute>
      <attribute id="DEFAULT_TEXT" order="3">
        <descriptions defaultDescription="DEFAULT_TEXT"/>
        <keyMapping columnObjectName="Script_View" columnName="DEFAULT_TEXT"/>
      </attribute>
      <attribute id="VALUE_OPTION_LIST_CODE" order="4">
        <descriptions defaultDescription="VALUE_OPTION_LIST_CODE"/>
        <keyMapping columnObjectName="Script_View" columnName="VALUE_OPTION_LIST_CODE"/>
      </attribute>
    </attributes>
