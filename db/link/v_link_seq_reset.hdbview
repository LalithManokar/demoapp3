schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.link::t_link\" 
         union all 
         select max(id) as id from \"sap.ino.db.link::t_link_h\" )";

depends_on_table = ["sap.ino.db.link::t_link",
                    "sap.ino.db.link::t_link_h"];