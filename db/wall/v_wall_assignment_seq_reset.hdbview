schema = "SAP_INO";
query = "select ifnull(max(id),0) + 1 as nextid from 
        (select max(id) as id from \"sap.ino.db.wall::t_wall_assignment\" 
         union all 
         select max(id) as id from \"sap.ino.db.wall::t_wall_assignment_h\" )";

depends_on_table = ["sap.ino.db.wall::t_wall_assignment",
                    "sap.ino.db.wall::t_wall_assignment_h"];
