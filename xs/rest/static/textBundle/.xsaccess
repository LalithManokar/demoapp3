{
    "rewrite_rules" : [{
        "source": "/(\\w+)_([a-z]{2,3})_[A-Z]{2}?_sappsd\\.properties",
        "target": "/text.xsjs?OBJECT=$1&LOCALE=$2&PSEUDO=1"
    },{
        "source": "/(\\w+)_([a-z]{2,3})(_[A-Z]{2}?)\\.properties",
        "target": "/text.xsjs?OBJECT=$1&LOCALE=$2$3"
    },{
        "source": "/(\\w+)_([a-z]{2,3})?\\.properties",
        "target": "/text.xsjs?OBJECT=$1&LOCALE=$2"
    },{
        "source": "/(\\w+).properties",
        "target": "/text.xsjs?OBJECT=$1"
    }],
    "authorization": null,
    "cache_control": "max-age=604800"
}